﻿@model IEnumerable<SachOnline.Models.NHAXUATBAN>
@{
    ViewBag.Title = "Sách theo Nhà Xuất Bản";
}
<li class="list-group-item active" style="font-weight:bold; text-align:center">SÁCH THEO NHÀ XUẤT BẢN</li>
@foreach (var item in Model)
{
    <a href="@Url.Action("SachTheoNhaXuatBan","SachOnline", new { iMaNXB = item.MaNXB})" class="list-group-item">@item.TenNXB</a>

}